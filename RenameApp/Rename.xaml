<Window x:Class="RenameApp.Rename"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RenameApp"
        mc:Ignorable="d"
        Title="Rename" Height="300" Width="300" Loaded="Window_Loaded" ContentRendered="Window_ContentRendered">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="10*"/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="FileListTextBlock" Margin="10,10,10,0" TextWrapping="Wrap"/>
        <TextBlock x:Name="outputNames" Margin="10,0,10,10" Grid.Row="2" TextWrapping="Wrap"/>
        <Separator Margin="10,2,10,0" Grid.Row="1"/>

    </Grid>
</Window>
